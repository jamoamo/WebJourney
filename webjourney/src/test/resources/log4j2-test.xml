<?xml version="1.0" encoding="UTF-8"?>
<Configuration strict="true" name="Logging">
  <Filter type="ThresholdFilter" level="trace"/>
 
  <Appenders>
    <Appender type="Console" name="STDOUT">
      <Layout type="PatternLayout" pattern="%d{dd MM yyyy HH:mm:ss.SSS} [%p] - [%C] %m %n"/>
    </Appender>
  </Appenders>
 
  <Loggers>
    <Logger name="io.github.jamoamo.webjourney.DefaultBrowserPool" level="debug" additivity="false">
      <AppenderRef ref="STDOUT"/>
    </Logger>
    <Root level="info">
      <AppenderRef ref="STDOUT"/>
    </Root>
  </Loggers>
 
</Configuration>