<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="debug" strict="true" name="Logging">
  <Filter type="ThresholdFilter" level="trace"/>
 
  <Appenders>
    <Appender type="Console" name="STDOUT">
      <Layout type="PatternLayout" pattern="%p %d{dd MM yyyy HH:mm:ss.SSS} [%C] %m %n"/>
    </Appender>
  </Appenders>
 
  <Loggers>
    <Root level="warn">
      <AppenderRef ref="STDOUT"/>
    </Root>
  </Loggers>
 
</Configuration>